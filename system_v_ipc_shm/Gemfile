# Gemfile

source "https://rubygems.org"

ruby "2.3.1"

# Development Utilities
gem 'awesome_print'  # Pretty print Ruby objects with proper indentation and colors
gem 'debug_me'       # A tool to print the labeled value of variables.

# System V Inter-process Communication and Shared Memory
gem 'SysVIPC'  # System V Inter-Process Communication for Ruby
gem 'sysvmq'   # Ruby wrapper for SysV Message Queues; used by Shopify

